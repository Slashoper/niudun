<source>
  type   kafka
  topics nginx-access
  hosts   61.174.14.218,61.174.14.219,61.174.14.220
  port   9092
  #format text
  format json
  client_id td-agent-input-nginx-access
  partition 5
  max_bytes           52428800
  max_wait_ms         5000
  min_bytes           1024
  socket_timeout_ms   50000
  add_prefix          abc
</source>

